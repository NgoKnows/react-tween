{
  "name": "react-tween",
  "version": "0.4.0-alpha.14",
  "description": "Tween animation for React components",
  "repository": "clari/react-tween",
  "license": "MIT",
  "main": "lib",
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "animation",
    "motion",
    "react",
    "react-component",
    "transition",
    "tween"
  ],
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.dev.js",
    "build-dist": "rm -rf dist && NODE_ENV=production webpack --config webpack.config.build.js",
    "build-lib": "rm -rf lib && NODE_ENV=production babel src --out-dir lib --only index.js,mergeDiff.js,toObject.js,TransitionGroup.jsx,Tween.jsx",
    "prepublish": "npm run build-lib && npm run build-dist"
  },
  "dependencies": {
    "d3-ease": "^1.0.0",
    "d3-interpolate": "^1.0.0",
    "d3-timer": "^1.0.0",
    "lodash.isequal": "^4.0.0",
    "lodash.isundefined": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.0",
    "babel-cli": "^6.0.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.0.0",
    "classnames": "^2.0.0",
    "css-loader": "^0.25.0",
    "d3-scale": "^1.0.0",
    "eslint": "^3.0.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "node-sass": "^3.0.0",
    "postcss-loader": "^1.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^3.0.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.0.0",
    "webpack-dev-server": "^1.0.0"
  }
}
